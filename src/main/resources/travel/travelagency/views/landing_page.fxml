<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.shape.*?>

<ScrollPane hbarPolicy="NEVER" prefWidth="1440" prefHeight="1024" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="travel.travelagency.controllers.LandingPageController">
    <content>
        <Pane prefWidth="1440" prefHeight="1022" >
            <children>
                <Rectangle fx:id="_bg__landing_page" width="1440" height="1024" fill="#FFFFFF">
                </Rectangle>
                <Rectangle fx:id="rectangle_2" width="720" height="1024" fill="#FFFFFF">
                </Rectangle>
                <ImageView fx:id="ellipse_2" pickOnBounds="true" preserveRatio="true" layoutX="209" layoutY="155"
                           fitWidth="301" fitHeight="185">
                    <Image url="@../../../drawable/ellipse_2.png"/>
                </ImageView>
                <ImageView fx:id="rectangle_3" pickOnBounds="true" preserveRatio="true" layoutX="720" layoutY="66"
                           fitWidth="720" fitHeight="1024">
                    <Image url="@../../../drawable/rectangle_3.png"/>
                </ImageView>
                <Group fx:id="loginButtonGroup" layoutY="680" layoutX="161" >
                    <Group fx:id="loginButtonFrame" onMouseClicked="#frame_3_onClick" >
                        <Button textAlignment="CENTER" layoutX="89.333333333333" layoutY="42" text="LOG IN" onAction="#_login_onClick">
                            <font>
                                <Font name="Mako" size="40" />
                            </font>
                        </Button>
                    </Group>
                </Group>

                <Group fx:id="usernameGroup" layoutY="452" layoutX="161">
                    <Group fx:id="usernameFrame">
                        <TextField fx:id="usernameTextField" layoutX="31.666666666667" layoutY="39" promptText="Username" >
                            <font>
                                <Font name="Poppins" size="32" />
                            </font>
                        </TextField>
                    </Group>
                </Group>

                <Group fx:id="passwordGroup" layoutY="546" layoutX="161" >
                    <Group fx:id="passwordFrame">
                        <PasswordField fx:id="passwordTextField" layoutX="31.666666666667" layoutY="39" promptText="Password" >
                            <font>
                                <Font name="Poppins" size="32" />
                            </font>
                        </PasswordField>
                    </Group>
                </Group>

                <Text fx:id="headerMessage" textAlignment="CENTER" fill="#000000" layoutX="209.33333333333" layoutY="400" wrappingWidth="337" text="Welcome back!" >
                    <font>
                        <Font name="Poppins" size="40" />
                    </font>
                </Text>

            </children>
        </Pane>
    </content>
</ScrollPane>